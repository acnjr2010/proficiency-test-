<div class="form-group">
  <%= f.label :name, "Nome" %><br>
  <%= f.text_field :name, class: "form-control", placeholder: "Nome Completo" %>
</div>

<div class="form-group">
  <%= f.label :register_number, "Número de Registro" %><br>
  <%= f.text_field :register_number, class: "form-control", placeholder: "Número do RG" %>
</div>

<div class="form-group">
  <%= f.label :status, "Status" %>
  <%= f.select :status, [['Ativo', '1'], ['Inativo', '0']], { include_blank: true }, { class: "form-control"}  %>
</div>
<br>

<div class="form-group">
  <%= f.submit  "Enviar", class: "btn btn-primary"%>
</div>
